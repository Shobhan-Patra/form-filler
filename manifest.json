{
  "manifest_version": 3,
  "name": "Job form filler",
  "version": "1.0",
  "description": "Auto-fills job application forms",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ]
}


